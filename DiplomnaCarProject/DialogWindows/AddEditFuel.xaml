<Window x:Class="DiplomnaCarProject.DialogWindows.AddEditFuel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiplomnaCarProject.DialogWindows"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        Title="AddEditFuel" Height="180" Width="300">
    <Grid>
        <TextBox materialDesign:HintAssist.Hint="Тип Гориво" Style="{StaticResource MaterialDesignFloatingHintTextBox}" FontFamily="Champagne &amp; Limousines" FontSize="18" Height="50" x:Name="textBoxFuel" HorizontalAlignment="Left" Margin="28,32,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="229" />
        <Button Foreground="White"  x:Name="b1" Content="Добави" HorizontalAlignment="Left" Margin="195,106,0,0" VerticalAlignment="Top" Width="87" Click="b1_Click"/>
        <Button IsCancel="True" Foreground="White" Content="Назад" HorizontalAlignment="Left" Margin="10,106,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
